{
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:flowtype/recommended",
      "plugin:react/recommended",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "rules": {
      "react/prop-types": 0,
      "no-console": 1
    },
    "plugins": ["flowtype", "react", "import", "jsx-a11y"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    }
  }
  